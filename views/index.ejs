<% include ./partials/header %>

<div class="ui main container">
  <h1>Blogs index</h1>
  <% blogs.forEach(function(blog){ %>
    <div>
      <h2><%= blog.title %></h2>
      <img src="<%= blog.image %>" >
      <p><%= blog.created %></p>
      <p><%= blog.body %></p>
      <a href="/blogs/<%= blog._id %>">Read More</a>
    </div>
  <% }) %>
</div>
<% include ./partials/footer %>
